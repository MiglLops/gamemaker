key_direita = ord("D");
key_esquerda = ord("A");
key_baixo = ord("S");
key_cima = ord("W");



if keyboard_check(key_direita)
{
x+=vel
image_xscale=-1
}

if keyboard_check(key_esquerda)
{
x-=vel	
image_xscale=1
}

if keyboard_check(key_baixo)
{
y+=vel	
}

if keyboard_check(key_cima)
{
y-=vel
}

if keyboard_check_pressed(vk_space)
{
	sprite_index=spr_mitocondriaS
}